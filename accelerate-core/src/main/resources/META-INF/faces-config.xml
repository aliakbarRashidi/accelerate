<?xml version="1.0" encoding="UTF-8"?>
<!-- This file is not required if you don't need any extra config. -->
<faces-config version="2.0"
   xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="
      http://java.sun.com/xml/ns/javaee
      http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

   <application>
    <navigation-handler>co.pishfa.accelerate.ui.navigation.NavigationHandlerBridge</navigation-handler>
   </application>
   
   <factory>
   	<exception-handler-factory>co.pishfa.accelerate.exception.AccelerateExceptionHandlerFactory</exception-handler-factory>
   </factory>
   
   <lifecycle>
   	<phase-listener>co.pishfa.accelerate.ui.phase.PhaseListenerCdiBridge</phase-listener>
   </lifecycle>

</faces-config>
